{
  "name": "Towns GitHub Bot",
  "url": "https://github.com/HereNotThere/bot-github",
  "hook_attributes": {
    "url": "https://your-bot.onrender.com/github-webhook",
    "active": true
  },
  "redirect_url": "https://your-bot.onrender.com/github-app/callback",
  "callback_urls": [
    "https://your-bot.onrender.com/github-app/callback"
  ],
  "setup_url": "https://your-bot.onrender.com/github-app/setup",
  "description": "Real-time GitHub notifications for Towns Protocol channels. Subscribe to repositories and receive instant webhook notifications for PRs, issues, commits, releases, and more.",
  "public": false,
  "default_permissions": {
    "contents": "read",
    "issues": "read",
    "pull_requests": "read",
    "metadata": "read",
    "actions": "read"
  },
  "default_events": [
    "pull_request",
    "push",
    "issues",
    "issue_comment",
    "release",
    "workflow_run",
    "pull_request_review",
    "pull_request_review_comment",
    "create",
    "delete",
    "installation",
    "installation_repositories"
  ]
}
